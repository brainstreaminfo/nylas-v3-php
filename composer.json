{
    "name": "brainstream/nylas-php",
    "description": "Nylas PHP SDK for api version 3",
    "keywords": [
        "nylas",
        "nylas-v3-api-php",
        "email",
        "contacts",
        "calendar",
        "php"
    ],
    "license": "MIT",
    "authors" : [
        {
          "name": "BrainStream Technolabs",
          "email": "support@brainstream.tech"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-json": "*",
        "guzzlehttp/guzzle": "6.1.1",
        "respect/validation": "1.1.29"
    },
    "autoload": {
        "psr-4": {
            "Nylas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration tests/phpunit.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
